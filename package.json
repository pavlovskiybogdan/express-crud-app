{
  "name": "test-task",
  "version": "1.0.0",
  "description": "Express rest api application",
  "main": "dev.js",
  "author": "Bogdan Pavlovskiy",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-node": "^0.0.1-security",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.3.0",
    "objection": "^3.0.1",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2"
  },
  "scripts": {
    "start": "rimraf ./dist && babel -d ./dist ./src && node ./dist/app.js",
    "dev": "nodemon dev.js",
    "lint": "eslint ./src"
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-register": "^6.26.0",
    "eslint": "^8.23.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.19"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "airbnb-base"
    ],
    "env": {
      "node": true
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "@": "./src"
          }
        }
      ]
    ]
  }
}
